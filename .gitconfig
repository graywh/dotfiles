[alias]
	co = checkout
	ci = commit
	st = !git --no-pager status
	br = branch
[color]
	branch = auto
	diff = false
	interactive = auto
	pager = false
	status = auto
[core]
	editor = vim
	pager = view -
	excludesfile = /home/graywh/.gitignored
[diff]
	rename = copy
[help]
	browser = dillo
[user]
	name = Will Gray
	email = graywh@gmail.com
[gui]
	fontdiff = -family Leonine\\ Sans\\ Mono -size 12 -weight normal -slant roman -underline 0 -overstrike 0
	spellingdictionary = none
[diff "ruby"]
	funcname = ^\\s*\\(\\(class\\|module\\|def\\|describe\\) .*\\)
[diff "python"]
	funcname = ^\\s*\\(\\(class\\|def\\) .*\\)
