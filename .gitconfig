[user]
	name = Will Gray
	email = graywh@gmail.com
[advice]
	statusHints = false
[alias]
	br = branch
	ci = commit
	co = checkout
	diffc = diff --cached
	diffci = diff --cached --ignore-all-space
	diffi = diff --ignore-all-space
	diffic = diff --cached --ignore-all-space
	ff = merge --ff-only
	log1 = log --oneline
	logg = log --oneline --graph
	pick = cherry-pick
	sl = !git --no-pager stash list
	st = status -sb
[branch]
	autosetupmerge = true
[color]
	diff = false
	pager = false
	ui = auto
[core]
	editor = vim
	pager = view -
	excludesfile = ~/.config/git/ignore
	attributesfile = ~/.config/git/attributes
[pager]
	status = false
[push]
	default = matching
[status]
	relativePaths = true
[diff]
	renames = copy
[diff "ruby"]
	funcname = ^\\s*\\(\\(class\\|module\\|def\\|describe\\|context\\) .*\\)
[diff "python"]
	funcname = ^\\s*\\(\\(class\\|def\\) .*\\)
[diff "vim"]
	function = ^\\s*\\(function .*\\)
[web]
	browser = sensible-browser
[gui]
	fontdiff = -family \"DejaVu Sans Mono\" -size 10 -weight normal -slant roman -underline 0 -overstrike 0
	spellingdictionary = none
	editor = gvim
[cola]
	fontdiff = DejaVu Sans Mono,10,-1,5,50,0,0,0,0,0
	fontui = DejaVu Sans,11,-1,5,50,0,0,0,0,0
