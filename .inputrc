set input-meta on
set output-meta on
set convert-meta off
set completion-ignore-case on
set mark-symlinked-directories on
set bell-style none
set editing-mode vi
set keymap vi-insert

"\e[1~": beginning-of-line
"\e[2~": quoted-insert
"\e[3~": delete-char
"\e[4~": end-of-line
"\e[A": history-search-backward
"\e[B": history-search-forward
"\e[C": forward-char
"\e[D": backward-char
